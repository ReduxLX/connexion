v>\r\n        <div onClick={() => handleDownvote()}>\r\n          <Downvote hasdownvoted={hasDownvoted.toString()} />\r\n        </div>\r\n      </RatingWrapper>\r\n    );\r\n  };\r\n\r\n  return (\r\n    <PostContentWrapper>\r\n      <PostHeader\r\n        onClick={() => {\r\n          history.push(`./post/${postId}`);\r\n        }}\r\n      >\r\n        {title.trim()}\r\n      </PostHeader>\r\n      <ChipGroup>\r\n        {categories.map((category, index) => (\r\n          <Chip key={index} category={category} />\r\n        ))}\r\n      </ChipGroup>\r\n      <PostBody>{renderBodyTextMemoized}</PostBody>\r\n      <Divider width=\"100%\" height=\"1px\" margin=\"0.8rem 0 0.3rem 0\" />\r\n      <Footer>\r\n        <FooterLeft>\r\n          <Avatar\r\n            alt=\"pic\"\r\n            src={photoURL}\r\n            style={{ width: \"25px\", height: \"25px\" }}\r\n          />\r\n          <p className=\"label\">Posted By</p>\r\n          <p>\r\n            <Poster className=\"PosterUsername\">\r\n              {truncateText(displayName, 15)}\r\n            </Poster>\r\n          </p>\r\n        </FooterLeft>\r\n        <FooterMiddle>\r\n          <Time>\r\n            <p>{convertSecondsToDate(timestamp.seconds)}</p>\r\n          </Time>\r\n          {showRating && <RatingControls>{renderRating()}</RatingControls>}\r\n        </FooterMiddle>\r\n        <FooterRight>\r\n          <GoComment style={{ width: \"20px\", height: \"20px\" }} />\r\n          <p>{comments}</p>\r\n        </FooterRight>\r\n      </Footer>\r\n    </PostContentWrapper>\r\n  );\r\n};\r\n\r\nconst PostContentWrapper = styled.div`\r\n  min-width: 250px;\r\n  max-width: 700px;\r\n  padding: 1.5em 1.5em 0.8em;\r\n  box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.08);\r\n  border-radius: 10px;\r\n  text-align: left;\r\n  word-wrap: break-word;\r\n  & > * {\r\n    margin-bottom: 0.5rem;\r\n  }\r\n`;\r\n\r\nconst PostBody = styled.div`\r\n  display: table;\r\n  table-layout: fixed;\r\n  width: 100%;\r\n  word-wrap: break-word;\r\n`;\r\n\r\nconst PostHeader = styled.h1`\r\n  margin-bottom: 5px;\r\n  transition: 0.2s;\r\n  font-size: 16px;\r\n  font-family: \"Raleway\";\r\n  cursor: pointer;\r\n  &:hover {\r\n    color: ${({ theme: { colors } }) => colors.main_dark};\r\n  }\r\n  @media (max-width: 768px) {\r\n    font-size: 16px;\r\n  }\r\n`;\r\n\r\nconst ChipGroup = styled.div`\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  & > * {\r\n    margin: 0.1rem 0.3rem 0.3rem 0;\r\n  }\r\n`;\r\n\r\nconst Footer = styled.div`\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding-right: 1rem;\r\n  color: #9c9c9c;\r\n  & > * {\r\n    margin-top: 0.2rem;\r\n    span {\r\n      opacity: 0.6;\r\n      color: ${({ theme: { colors } }) => colors.main};\r\n    }\r\n    & > *,\r\n    & > * > * {\r\n      font-size: 13px;\r\n    }\r\n    @media (max-width: 768px) {\r\n      & > *,\r\n      & > * > * {\r\n        font-size: 12px;\r\n      }\r\n    }\r\n  }\r\n`;\r\n\r\nconst FooterLeft = styled.div`\r\n  display: flex;\r\n  align-items: center;\r\n  & > * {\r\n    margin-right: 0.5rem;\r\n  }\r\n  .label {\r\n    @media (max-width: 768px) {\r\n      display: none;\r\n    }\r\n  }\r\n  .PosterUsername {\r\n    @media (max-width: 360px) {\r\n      display: none;\r\n    }\r\n    &:hover {\r\n      opacity: 1;\r\n      color: ${({ theme: { colors } }) => colors.main};\r\n    }\r\n  }\r\n`;\r\n\r\nconst FooterMiddle = styled.div`\r\n  display: flex;\r\n  flex: 1;\r\n  justify-content: flex-end;\r\n  padding-right: 5px;\r\n`;\r\n\r\nconst Time = styled.div`\r\n  display: flex;\r\n  align-items: center;\r\n  @media (max-width: 768px) {\r\n    display: none;\r\n  }\r\n`;\r\n\r\nconst RatingControls = styled.div`\r\n  display: none;\r\n  align-items: center;\r\n  @media (max-width: 768px) {\r\n    display: flex;\r\n  }\r\n  & > * {\r\n    margin-right: 0.5rem;\r\n  }\r\n`;\r\n\r\nconst FooterRight = styled.div`\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center